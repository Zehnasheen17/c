#include<stdio.h>
int main()
{
    int i,j,fact,n;
    int a[]={3,5,2};
    n=sizeof(a)/sizeof(int);
    int sum=0;
    for(i=0;i<n;i++)
    { 
        fact=1;
        for(j=1;j<=a[i];j++)
        {
        fact=fact*j;
        }
        sum=sum+fact;
    }
    printf("%d",sum);

    return 0;
}
Output:
128
