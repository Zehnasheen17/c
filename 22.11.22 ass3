address=(input('enter address: '))
dper=float(input('enter degree percentage: '))
print('STUDENT DATA:')
print('sno : ',sno)
print('name: ',name)
print('group: ',group)
print('gender: ',gender)
print('phno: ',phno)
print('address: ',address)
print('degree percentage: ',dper)


